{
  "manifest_version": 2,

  "name": "TicTacToe",
  "description": "A tic tac toe for chrome",
  "version": "1.0",
  "content_security_policy": "style-src 'unsafe-inline'; script-src 'self' https://connect.facebook.net; object-src 'self'",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "icon.html",
	"default_title": "Play!"
  },
  "permissions": [
	"tabs"
  ]
}